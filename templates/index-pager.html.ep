    <a href="/?timestamp=<%= $pager->prev %>"><span class="arrow">&larr; </span>Prev</a>
    % } else {
    <span class="arrow">&larr; </span>Prev
    % }

    % if ($pager->next) {
    <a href="/?timestamp=<%= $pager->next %>">Next<span class="arrow"> &rarr;</span></a>
    % } else {
    Next<span class="arrow"> &rarr;</span>
    % }
  </nav>
